[
    {
        "rule": "Check MyObject.zip is a proper Zipcode",
        "conditions": [
            {
                "property":"MyObject.zip",
                "condition":"zipcode"
            }
        ],
        actions: [
            {
                "callOnFalse": "rejectZip",
                "args": [ "MyObject.zip" ]
            },
            {
                "callOnTrue": "acceptZip"
                "args": []
            }
        ]
    },
    {
        "rule": "Check MyObject.name equals Foo",
        "conditions": [
            {
                "property":"MyObject.name",
                "condition":"equal",
                "value":"Foo"
            }
        ],
        actions: [
            { "returnOnFalse": "No, name was not Foo" },
            { "returnOnTrue": "Yes, name is Foo" }
        ]
    }
]